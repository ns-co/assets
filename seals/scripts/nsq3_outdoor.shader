textures/nsq3_democritus/waterfall
{
	qer_editorimage textures/nsq3_democritus/waterfall-ogun-01_qer.tga
	surfaceparm trans
	surfaceparm nonsolid
	surfaceparm water
	{
		map textures/nsq3_democritus/waterfall-ogun-02.tga
		blendfunc add
		rgbGen const ( 0.501961 0.501961 0.501961 )
		tcMod scroll -0.1 -0.9
	}
	{
		map textures/nsq3_democritus/waterfall-ogun-01.tga
		blendfunc add
		rgbGen const ( 0.501961 0.501961 0.501961 )
		tcMod scroll 0.1 -1
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

