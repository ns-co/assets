textures/nsq3_sub_crates/crate_space_explo
{	
	surfaceparm metalsteps
	{
		map textures/nsq3_sub_crates/crate_space_explo.tga
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
	}
}

