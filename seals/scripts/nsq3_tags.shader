// textures/nsq3_urban/concrete_2

textures/nsq3_tags/concrete_2_tag_01
{
	qer_editorimage textures/nsq3_urban/concrete_2.tga
	{
		map textures/nsq3_urban/concrete_2.tga
		rgbgen identity  
	}
	{
		map textures/nsq3_graph/tag_01.tga
		blendFunc blend
		rgbgen identity  
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
