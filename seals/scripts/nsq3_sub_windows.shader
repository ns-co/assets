// Shader by Ogun, textures by Subverse

textures/nsq3_sub_windows/backyard_1
{	
	surfaceparm glasssteps
	{
		map textures/nsq3_sub_windows/backyard_1.tga
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
	}
}

