textures/krut/cement_1_grimy
{
	surfaceparm metalsteps
	qer_editorimage textures/krut/cement_1_grimy.tga
	{
		map textures/krut/cement_1_grimy.tga
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
	}
}

