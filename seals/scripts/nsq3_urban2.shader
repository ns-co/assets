textures/nsq3_urban2/stainedglass05_lightfilter
{
	qer_editorimage textures/nsq3_urban2/stainedglass05.tga
	surfaceparm lightfilter
	surfaceparm trans
	surfaceparm glasssteps
	sort additive
	{
		map textures/nsq3_urban2/stainedglass05.tga
		rgbgen identity
		blendfunc add
		depthWrite
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
	}
}

