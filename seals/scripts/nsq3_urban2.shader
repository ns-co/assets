	surfaceparm 	alphaShadow
	surfaceparm 	playerclip
	surfaceparm	nonsolid
	{
		map textures/nsq3_urban/rail_03.tga
		alphaFunc GT0
		depthWrite
		blendfunc blend
		rgbgen identity      
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		depthFunc equal
		rgbgen identity      
	}
}

