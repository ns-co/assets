textures/nsq3_mansion/gothic-stained-glass-1
{
	qer_editorimage textures/nsq3_mansion/gothic-stained-glass-1.jpg
	surfaceparm trans
	cull none
	surfaceparm lightfilter
	surfaceparm glass
	surfaceparm noimpact
	qer_trans 0.6
	{
		map textures/nsq3_mansion/gothic-stained-glass-1.jpg
		blendfunc gl_dst_color gl_zero 
	}
}

