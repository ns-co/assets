textures/nsq3_mansion/banner1
{
	tessSize 64
	deformVertexes wave 194 sin 0 3 0 .4
	deformVertexes normal .5 .1
	surfaceparm nomarks
	cull none
	nopicmip
	nomipmaps
	surfaceparm alphaShadow
	surfaceparm noimpact
	surfaceparm nonsolid
	{
		map textures/nsq3_mansion/banner1.tga
		blendFunc blend
		alphaFunc GT0
		rgbGen identity
		depthWrite
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
		depthFunc equal
	}
}

