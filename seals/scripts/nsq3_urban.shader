textures/nsq3_urban/tv_anim_01
{
	qer_editorimage textures/nsq3_urban/tv_anim_01.tga	
	q3map_surfaceLight 500
	surfaceparm	glass
	{
		map textures/nsq3_urban/tv_anim_01.tga	
		tcMod turb 0 .1 0 .05
	}
        {
                map textures/sfx/snow.tga
                tcmod scale .5 .5
                tcmod scroll  9 0.3
                rgbGen identity
		blendfunc add
        }
	{
		map textures/nsq3_urban/tv.tga
		rgbGen identity
		blendfunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
	}
}
