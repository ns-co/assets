textures/nsq3_urban/rail_01
{
	surfaceparm 	noimpact
	surfaceparm 	trans
	surfaceparm 	alphaShadow
	surfaceparm 	playerclip
	surfaceparm	nonsolid
	{
		map textures/nsq3_urban/rail_01.tga
		alphaFunc GT0
		depthWrite
		rgbgen identity   
		blendfunc blend   
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		depthFunc equal
		rgbgen identity      
	}
}

