textures/nsq3_schakal4/fence_01
{
        qer_editorimage textures/nsq3_schakal4/fence_01.tga
    	surfaceparm trans
	surfaceparm alphashadow
	surfaceparm playerclip
	surfaceparm metalsteps
	cull none
	{
		map textures/nsq3_schakal4/fence_01.tga
		blendFunc GL_ONE GL_ZERO
		alphaFunc GE128
		depthWrite
		rgbGen identity
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
		depthFunc equal
	}
}

